{
    "version": 2,
    "builds": [
        {
            "src": "server.ts",
            "use": "@vercel/node"
        }
    ],
    "functions": {
        "server.ts": {
            "maxDuration": 60
        }
    },
    "routes": [
        {
            "src": "/api/v1/(.*)",
            "dest": "server.ts"
        },
        {
            "src": "/test",
            "dest": "server.ts"
        }
    ]
}